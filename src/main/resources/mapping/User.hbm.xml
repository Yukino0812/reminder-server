<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="me.yukino.reminderserver.record.vo.User" table="user" schema="user_info">
        <id name="name">
            <column name="name" sql-type="varchar(100)" length="100"/>
        </id>
        <property name="password">
            <column name="password" sql-type="char(32)" length="32"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(200)" length="200"/>
        </property>
    </class>
</hibernate-mapping>